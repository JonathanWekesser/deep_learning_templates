{
  "name": "DL Templates (TF on AMD/ROCm)",
  "dockerComposeFile": ["${localWorkspaceFolder}/docker-compose.yaml"],
  "service": "tf-amd",
  "runServices": ["tf-amd"],
  "workspaceFolder": "/workspace",
  "updateRemoteUserUID": true,
  "shutdownAction": "none",
  "overrideCommand": false,
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  }
}
